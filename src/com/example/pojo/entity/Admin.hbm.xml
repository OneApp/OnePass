<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>   
    <class name="com.example.pojo.entity.Admin" table="onepass_admin">
        <id name="adminId" type="java.lang.Integer">
            <column name="adminId" />
            <generator class="assigned" />
        </id>
        <property name="adminName" type="java.lang.String">
            <column name="adminName" />
        </property>
        <property name="adminPassword" type="java.lang.String">
            <column name="adminPassword" />
        </property>
        <property name="adminJurisdiction" type="java.lang.Boolean">
            <column name="adminJurisdiction" />
        </property>
        <property name="adminPhone" type="java.lang.String">
            <column name="adminPhone" />
        </property>
        <property name="adminEmail" type="java.lang.String">
            <column name="adminEmail" />
        </property>
    </class>
</hibernate-mapping>
